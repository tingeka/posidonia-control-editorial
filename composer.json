{
  "name": "revistaposidonia/editorial-control",
  "description": "Plugin for editorial control in Revista Posidonia",
  "license": "MIT",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/tingeka/revistaposidonia-editorial-control",
  "authors": [
    {
      "name": "Martín García"
    }
  ],
  "scripts":{
    "scope": "bash bin/scope.sh"
  },
  "require": {
    "php": ">=8.2",
    "10up/wp-framework": "~1.2.0",
    "monolog/monolog": "^3.9"
  },
  "autoload": {
    "psr-4": {
      "RevistaPosidonia\\EditorialControl\\": "src/"
    }
  },
  "require-dev": {
    "humbug/php-scoper": "^0.18.17",
    "sniccowp/php-scoper-wordpress-excludes": "^6.8"
  }
}
